{
  "redirects": [
    {
      "source": "/contact.html",
      "destination": "/contact-us.html",
      "permanent": true
    },
    {
      "source": "/teams.html",
      "destination": "/team.html",
      "permanent": true
    }
   
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/404.html",
      "has": [
        { "type": "missing" }
      ]
    }
  ]
}
